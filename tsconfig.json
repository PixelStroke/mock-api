{
    "compilerOptions": {
        "target": "ES2020",
        "module": "Node16",
        "esModuleInterop": true,
        "strict": true,
        "baseUrl": "src",
        "rootDir": "src",
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
        "removeComments": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*",
        "src/**/*.json"
    ],
  }